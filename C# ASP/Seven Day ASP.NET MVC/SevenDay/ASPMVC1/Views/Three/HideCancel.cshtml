<!--- 隐藏提交窗体 --->
<!---  将提交按钮改为正常按钮，并且使用JavaScript脚本代码 --->
<!---  修改action的submit 按钮需要放在form内部 --->
<!---  调用submit 按钮需要可以不放在form内部 --->

@{
    Layout = null;
}
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>PrintSchool</title>
</head>
<body>
    <div>
        <form action="/Three/GetCancelClick" id="cancelForm" method="get" style="display:none">
            Name  : <input type="text" id="TxtSchoolName" name="Name" value="" /><br />
            Province: <input type="text" id="TxtProvince" name="Address.Province" value="" /><br />
            City: <input type="text" id="TxtCity" name="Address.City" value="" /><br />
            <input type="submit" name="UP" value="UP" />
        </form>
        <input type="button" name="BtOK" value="HideCancel" onclick="document.getElementById('cancelForm').submit()" />
    </div>
</body>
</html>

