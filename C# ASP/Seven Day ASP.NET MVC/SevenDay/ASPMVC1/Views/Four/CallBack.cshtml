@using ASPMVC1.Controllers
@model FourEntity

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>CallBack</title>
</head>
<body>
    <div>
        <form action="" id="upf" method="post">
            Name  : <input type="text" id="TxtName" name="Name" value=@Model.Name><br />
            PassWord: <input type="text" id="TxtPassWord" name="PassWord" value=@Model.PassWord><br />
            <input type="submit" name="BToK" value="ModelBind2" onclick="document.getElementById('upf').action='/Four/CallBack'" />
        </form>
    </div>
</body>
</html>
