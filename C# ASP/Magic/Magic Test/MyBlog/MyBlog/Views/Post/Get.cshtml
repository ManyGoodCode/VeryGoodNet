
@{
    ViewBag.Title = "文章内容";
}

<div class="row">
    <div class="col-lg-8 col-md-10 mx-auto">
        <div class="post-heading">
            <h1>@ViewBag.Title</h1>
            <span class="meta">
                Posted by
                <a href="#">@ViewBag.Post.Author</a>
                @ViewBag.Post.CreateDate.ToString("yyyy-MM-dd HH:mm")
            </span>
        </div>
    </div>
</div>


